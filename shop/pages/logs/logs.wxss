.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.log-item {
  margin: 10rpx;
}
page{
  display: flex;
  height: 100%;
  margin: auto;
  justify-content: center;
  align-items: center;
}
.logsBox{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
}
.logsBox image{
  width: 160rpx;
  height: 80rpx;
}
.logsBox button{
  margin: 20rpx 0px;
  background-color: #1aad19;
  color: white;
  border: 1px solid #1aad19;
  width: 600rpx !important;
}
/* 添加 */
.btnWrap{
 display: flex;
 justify-content: space-around;
 width: 100%;
 flex-direction: column;
}
.btnWrap button{
  text-align: center;
}
.btnWrap .cancelBtn{
  background-color: white;
  color: black;
  border-color: white;
  margin-top: 0rpx;
}
