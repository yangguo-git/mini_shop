@import '../productDetail/productDetail.wxss';
.searchName{
  font-size: 16px;
  height: 61rpx;
  border: 1px solid #dddddd;
  width: 65%;
  border-radius: 50px;
  padding-left: 20rpx;
  line-height: 61rpx;
  margin-top: 20rpx;
  margin-left: 40rpx;
  background-color: white;
}
.product-none{
  padding: 0 30rpx;
  height: 450rpx;
  line-height: 450rpx;
  text-align: center;
  color: #cccccc;
}
button{
  margin-top: 30rpx;
  margin-bottom: 30rpx;
}
.button-sp-area{
  margin: 0 auto;
  width: 60%;
}
.mini-btn{
  margin-right: 10rpx;
  font-size: 30rpx;
  font-weight: 1;
  padding: 0;
  border: 1px solid #dddddd;
  width: 140rpx!important;
  padding: 0 0;
  height: 65rpx;
  border-radius: 50px;
  margin: 0;
  margin-top: 20rpx;
  background-color: white;
  line-height: 65rpx;
}
.m-indexHd{
  height: 56px;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.zw-proli {
  display: flex;
  padding-bottom: 12px;
  margin-top: 25rpx;
  border-bottom: 1px solid #ebebeb;
  width: 90%;
  margin: 0 auto;
  margin-top: 15rpx;  
}
.zw-proli-img {
  width: 200rpx;
  height: 200rpx;
}
.zw-proli image {
  width: 100%;
  height: 100%;
  display: block;  
}
.zw-proli-content {
  padding-left: 25rpx;
}
.zw-proli-tips {
   display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  text-align: left;
  overflow: hidden;
  /* white-space: nowrap; */
  /* -o-text-overflow: ellipsis; */
  /* text-overflow: ellipsis; */
  font-size: 30rpx;
  /* background-color: #f7f7f7; */
  font-weight: 600;
}
.zw-proli-info {
  text-align: left;
  width: 100%;
  height: 114rpx;
  align-items: center;
  padding-top: 10rpx;
}
.zw-proli-info .price-wrap {
  margin-top: 15px;
  color: #f52b2b;
  display: flex;
  /* justify-content: space-between; */
  width: 430rpx;
  align-items: center;
  position: relative;
}
.zw-proli-info .price {
  font-size: 30rpx;
  font-weight: 600;
}
.cate-add{
  width: 40rpx;
  height: 40rpx;
}
.littleTips{
  font-size: 26rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  color: #a6a6a6;
}
.zw-proli-img image{
width: 200rpx;
height: 200rpx;
}
.huaXian{
  font-size: 24rpx;
  text-decoration: line-through;
  padding-left: 12rpx;
  color: #a6a6a6;
  padding-top: 6rpx;
}
.addImage{
 position: absolute;
 right: 0px;
}
.product-content{
  background-color: white;
  padding-top: 6.5rpx;
}
.commodity_attr_list{
  bottom: 0;
  z-index: 1024;
  padding-bottom: 30rpx;
  max-height: 70%;
}
.addShopCard{
  color: white!important;
  border-radius: 50rpx;
  margin-top: 30rpx;
  background: linear-gradient(to right, #25d09f, #27d98c);
}
.dialog{
  z-index: 999;
}
.cate-card{
  margin-top: 25rpx;
}
.selectArrText{
  margin-top: 20rpx;
  color: #999999;
  padding-left: 10rpx;
}
.attr_value_active{
  color: #08c160;
  padding: 0 30rpx;
  border: 1rpx solid #08c160;
  background-color: rgba(147,222,176,0.2);
}












