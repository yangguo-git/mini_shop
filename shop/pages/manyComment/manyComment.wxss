.many-box {
    font-size: 30rpx;
}

.many-left image {
    width: 135rpx;
    height: 135rpx;
}

.item-box {
    display: flex;
    margin: 10rpx;
    background-color: white;
}

.many-right {
    flex: 1;
    display: flex;
    flex-direction: column;
}

.rightBottom {
    align-self: flex-end;
    margin-top: 45rpx;
    margin-right: 15rpx;
    display: flex;
}

.many-left {
    padding-top: 15rpx;
    padding-left: 15rpx;
}

.rightTop {
    margin-top: 5rpx;
    margin-left: 10rpx;

}

.rightBottom text {
    border: 1px solid #d6d6d6;
    border-radius: 30rpx;
    padding: 5rpx 20rpx;
    font-size: 28rpx;
    text-align: center;
    width:115rpx;
}