page{
  height: 100%;
}
.right image{
  width: 25rpx;
  height: 38rpx;
}
.m-shipAddress .item .phone, .m-shipAddress .item .uname {
  font-size: 32rpx;
  margin-bottom: 0.0500025rpx;
  line-height: 1.5;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  display: flex;
  justify-content: space-between;
}
.m-shipAddress .item .addr {
  font-size: 30rpx;
  color: #666;
  margin-top: 15rpx;
}
.orderReceiveList .m-shipAddress .item {
  padding-left: 0;
  margin-left: 30rpx;
}
.orderReceiveList .m-shipAddress .item {
  padding-left: 0;
  margin-left: 30rpx;
  margin-right: 30rpx;
  margin-bottom: 20rpx;
  background-color: #fff;
}
.container  {
  overflow:hidden;
  background-color: #f6f6f6;
  height: 100%;
}
.ma-layort {
  display: block;
  margin-top: 19rpx;
}
.m-shipAddress .item {
  display: flex;
  padding: 32rpx 0 24rpx 30rpx;
}
.m-shipAddress .item .middle {
  padding-left: 15rpx;
  flex: 1;
}
.m-shipAddress .item .right {
  width: 75rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.orderReceiveList{
  width: 100%;
}
.w-button {
  height: 75rpx;
  line-height: 75rpx;
  text-align: center;
  color: #666;
  display: block;
  width: 100%;
  border: 0;
  background-color: #fff;
  font-size: 24rpx;
  margin-top: 22rpx;
}
.orderReceiveList .button-wrap {
  height: 145rpx;
  position: fixed;
  /* left: 5%; */
  bottom: 0;
  width: 100%;
  /* border-top: 1px solid #ccc; */
  background-color: #fff;
  display: flex;
  align-items: center;
}
.orderReceiveList .w-button {
  width: 71%;
  background: #f4f4f4;
  color: #fff;
  border-radius: 2px;
  z-index: 4;
  padding:0;
  background: linear-gradient(225deg,#25cfa1,#27d98c);
  border-radius: 50rpx;
  height: 75rpx;
  line-height: 75rpx;
  font-size: 28rpx;
  font-weight: 100;
}
.buttom{
  width: 100%;
  height: 400rpx;
  line-height: 400rpx;
  text-align: center;
  color: #999;
}
.adressBox{
  display: flex;
}
