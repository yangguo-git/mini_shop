/* pages/member/orderList/orderList.wxss *//* pages/member/orderList/orderList.wxss */
.m-c-orderList{
  display: flex;
  justify-content: space-between;
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 30rpx;  
}
.m-c-orderContent{
  background-color: #fff;
  margin-bottom: 15rpx;
  width: 100%;
}
.m-c-orderList-left{
  display: flex;
  align-items: center;
}

.item {
  margin-left: 19rpx;
  margin-top: 30rpx;
  display: flex;
  font-size: 22rpx;
  width: 100%;
  position: relative;
  align-items: center;

}
.od-item-right{
  width: 100%;
}
.cate-img-left {
  width: 190rpx;
  height: 190rpx;
  margin-right: 20px;
}
.cate-name{
  font-size: 30rpx;
  font-weight: 600;
}
.cate-sub{
  color: #a0a0a0;
  font-size: 28rpx;
  margin: 20rpx 0 10rpx 0;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}
.cate-price{
  color: #ff541f;
  font-weight: 600;
  font-size: 35rpx;
}
.orderId{
  flex: 1;
  height: 1.0500025rpx;
  line-height: 1.0500025rpx;
  margin-right: 30rpx;
  font-size: 33rpx;
}
.od-status{
  display: inline-block;
  margin-right: 30rpx;
  color: #ff541f;
}
.od-googs .hd, .order-item .hd {
  height:80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
}
.order-item .bd {
  /* display: flex; */
  padding-bottom: 24rpx;
}
.order-item {
  margin-top: 15rpx;
  padding-left: 30rpx;
  background-color: #fff;
  font-size: 27rpx;
}
.order-item .btns, .orderDetail .btns {
  height: 100rpx;
  border-top: 1px solid #dddddd;
  
  }
.order-item .btns, .orderDetail .btns {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  }
  .w-button {
    text-align: center;
    color: #666;
    display: block;
    background-color: #fff;
}
  .order-item .btns .w-button, .orderDetail .btns .w-button {
    height: 55rpx;
    font-size: 27rpx;
    line-height: 55rpx;
    width: 160rpx;
    border: 1px solid #d6d6d6;
    border-radius: 50px;
    margin-top: 0;
    margin-right: 22rpx;
}
.greybg {
  /* background-color: #f4f4f4; */
  min-height:100%;
}
.m-orderList{
  width: 100%;
}
.od-item-right-text{
  display: flex;
  justify-content: space-between;
  padding-right: 30rpx;
  align-items: center;
}
.m-c-orderList-left text{
  font-weight: 600;
}
.buttom{
  width: 100%;
  height: 400rpx;
  line-height: 400rpx;
  text-align: center;
  color: #999;
  background-color: #f0f0f8;
}
.cate-count{
  align-self: center;
  padding-top: 10rpx;
  font-size: 28rpx;
  color: #b3b3b3;
}
.payBtn{
  color: #25c873;
  border-color: #25c873 !important;
  background: white;
}
.lookBtn{
  color: black;
  /* border-color: black !important; */
}

